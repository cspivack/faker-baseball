{
    "name": "cspivack/faker-baseball",
    "description": "Use fakerphp/faker to generate data from the Fighting Baseball roster.",
    "type": "library",
    "require": {
        "fakerphp/faker": "^1.9.1",
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Faker\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faker\\Tests\\Provider\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "cspivack",
            "email": "spivack.craig@gmail.com"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "./vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse src --level=5"
    }
}
